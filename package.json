{
  "name": "starter-kit-express",
  "version": "0.0.1",
  "private": true,
  "description": "Some stuff goes here",
  "scripts": {
    "start": "nodemon index.js",
    "lint": "eslint ./",
    "security": "nsp check",
    "test": "jest",
    "precommit": "npm-run-all --parallel lint test",
    "prepush": "npm-run-all --parallel lint test security",
    "ci": "npm run prepush && snyk test https://github.com/maxmalov/starter-kit-express"
  },
  "engines": {
    "node": ">=6.3.0",
    "npm": ">=3.10.3"
  },
  "dependencies": {
    "body-parser": "1.15.2",
    "bunyan": "1.8.1",
    "bunyan-prettystream": "0.1.3",
    "cookie-parser": "1.4.3",
    "express": "4.14.0",
    "express-session": "1.14.0",
    "http-errors": "1.5.0",
    "http-status": "0.2.3",
    "lodash": "4.15.0",
    "mkdirp": "0.5.1",
    "nconf": "0.8.4",
    "nconf-yaml": "1.0.2"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "chai-as-promised": "5.3.0",
    "chai-json-schema": "1.3.0",
    "eslint": "3.4.0",
    "eslint-config-airbnb-base": "5.0.3",
    "eslint-plugin-import": "1.14.0",
    "husky": "0.11.7",
    "jest": "15.1.1",
    "nodemon": "1.10.2",
    "npm-run-all": "3.1.0",
    "nsp": "2.6.1",
    "sinon": "1.17.5",
    "sinon-chai": "2.8.0",
    "snyk": "1.19.1",
    "supertest": "2.0.0",
    "supertest-as-promised": "4.0.0"
  },
  "jest": {
    "testEnvironment": "node",
    "testPathDirs": [
      "tests"
    ],
    "testRegex": "-test\\.js$",
    "collectCoverage": true,
    "coveragePathIgnorePatterns": [
      "<rootDir>/tests",
      "/node_modules/"
    ]
  }
}
